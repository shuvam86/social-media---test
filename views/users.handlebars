<div class="container-fluid">
    
    <div class="row">
         {{#each users}}              {{!-- each method will return all the users from inside array users --}}
        <div class="col-sm-3">
            <div class="card">
                <img class="card-img-top" src="{{image}}">
                <div class="card-body">
                    <a href="/user/{{id}}"><h5 class="card-title">{{fullname}}</h5> </a>
                    <p class="card-title">{{location}}</p>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>